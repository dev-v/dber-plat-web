{
  "entry": "src/index.js",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
         ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    },
    "proxy": {
      "*/api/plat": {
        "target": "http://localhost:8080/",
        "changeOrigin": true,
        "pathRewrite": { "^/api/plat" : "" }
      }
    }
  }
}
